{
  "expo": {
    "name": "Grocery App",
    "slug": "grocery-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "groceryappfeexpo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sujal.groceryapp",
      "config": {
        "googleMaps": {
          "apiKey": "process.env.IOS_GOOGLE_MAPS_API"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sujal.groceryapp",
      "edgeToEdgeEnabled": true,
      "config": {
        "googleMaps": {
          "apiKey": "process.env.ANDROID_GOOGLE_MAPS_API"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Grocery App to use your location.",
          "isAndroidBackgroundLocationEnabled": true,
          "isIOSBackgroundLocationEnabled": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "19bfb58a-d9d7-4e43-934f-caa0f2a52990"
      }
    }
  }
}